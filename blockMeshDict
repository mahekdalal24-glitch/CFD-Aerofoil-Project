FoamFile
{
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


convertToMeters 1;

vertices (
    (-1.2 -0.5 -0.5)
    (2.0 -0.5 -0.5)
    (2.0 0.6 -0.5)
    (-1.2 0.6 -0.5)
    (-1.2 -0.5 0.5)
    (2.0 -0.5 0.5)
    (2.0 0.6 0.5)
    (-1.2 0.6 0.5)

);

blocks (
    hex (0 1 2 3 4 5 6 7)
    (150 100 10)
    simpleGrading (1 1 1)
);

edges 
( 
);

boundary 
(
    inlet
    {
        type patch;
        faces ((0 4 7 3));
    }
    outlet
    {
        type patch;
        faces ((1 2 6 5));
    }
    top
    {
        type patch;
        faces ((2 3 7 6));
    }
    bottom
    {
        type patch;
        faces ((0 1 5 4));
    }
    frontAndBack
    {
        type patch;
        faces ((0 3 2 1) (4 5 6 7));
    }
);

mergePatchPairs ( );




